{
  "$schema": "https://json.schemastore.org/swcrc",
  "env": {
    "targets": [
      "chrome >= 80",
      "safari >= 11"
    ]
  },
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "dynamicImport": false
    },
    "target": "es2020"
  },
  "module": {
    "type": "es6",
    "strict": true,
    "noInterop": true
  },
  "minify": false
}
